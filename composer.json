{
    "name": "gmsantos/inspiring",
    "description": "Alternative inspiring quotes for Laravel 5+",
    "keywords": ["laravel", "package", "console"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "gmsantos",
            "email": "gabriel.ms1@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5|^6"
    },
    "autoload": {
        "psr-4": {
            "Gmsantos\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Gmsantos\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "php vendor/bin/phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
